{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Shake",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.shake",
          "match": "\\b(if|else|while|for|return|do)\\b"
        },
        {
          "name": "keyword.type",
          "match": "\\b(byte|short|int|long|float|double|char|boolean|void|dynamic)\\b"
        },
        {
          "name": "keyword.access",
          "match": "\\b(public|private|protected)\\b"
        },
        {
          "name": "keyword.modifier",
          "match": "\\b(static|final|abstract|native|synchronized|operator|override|unsigned|const|synchronized)\\b"
        },
        {
          "name": "keyword.declaration",
          "match": "\\b(class|interface|enum|object|constructor)\\b"
        },
        {
          "name": "keyword.file",
          "match": "\\b(package|import)\\b"
        },
        {
          "name": "keyword.operator",
          "match": "\\b(new|as|this|super)\\b"
        },
        {
          "name": "keyword.literal",
          "match": "\\b(true|false|null)\\b"
        },
        {
          "name": "keyword.other",
          "match": "\\b(break|continue|throw|try|catch|finally|assert)\\b"
        },
        {
          "name": "keyword.inheritance",
          "match": "\\b(extends|implements)\\b"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.shake",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.shake",
          "match": "\\\\."
        }
      ]
    }
  },
  "scopeName": "source.shake"
}
